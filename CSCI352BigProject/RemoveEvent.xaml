<Window x:Class="CSCI352BigProject.RemoveEvent"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CSCI352BigProject"
        mc:Ignorable="d"
        Title="Remove Event
" Height="450" Width="350">
    <Grid>
        <Button x:Name="removeEvent" Content="Remove Event" HorizontalAlignment="Left" Margin="116,366,0,0" VerticalAlignment="Top" Width="88" Click="removeEvent_Click"/>
        <Label Content="Date (Ex. December 12)" HorizontalAlignment="Left" Margin="49,62,0,0" VerticalAlignment="Top"/>
        <Label Content="Event Name (Ex. Eye Doctor)" HorizontalAlignment="Left" Margin="49,155,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="dateBox" HorizontalAlignment="Left" Height="23" Margin="49,92,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="228"/>
        <TextBox x:Name="eventBox" HorizontalAlignment="Left" Height="23" Margin="49,186,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="228" SpellCheck.IsEnabled="True"/>

    </Grid>
</Window>
